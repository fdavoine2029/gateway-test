<div class="container">
    <div class="btn-group" role="group" aria-label="Un groupe de boutons">
        {% if sklblOrder.sklblStatus == 0 %}
            <button type="button" class="btn btn-primary" onclick="step_1({{sklblOrder.id}})">Intégration fichier client</button>
            <button type="button" class="btn btn-secondary" disabled>Calcul des quantités</button>
            <button type="button" class="btn btn-secondary" disabled>Génération OF</button>
            <button type="button" class="btn btn-secondary" disabled>Génération Fx</button>
            
        {% endif %}
        {% if sklblOrder.sklblStatus == 1 %}
            <button type="button" class="btn btn-success"  onclick="step_1({{sklblOrder.id}})">Intégration fichier client</button>
            <button type="button" class="btn btn-primary"  onclick="step_2({{sklblOrder.id}})">Calcul des quantités</button>
            <button type="button" class="btn btn-secondary" disabled>Génération OF</button>
            <button type="button" class="btn btn-secondary" disabled>Génération Fx</button>
        {% endif %}
        {% if sklblOrder.sklblStatus == 2 %}
            <button type="button" class="btn btn-success"  onclick="step_1({{sklblOrder.id}})">Intégration fichier client</button>
            <button type="button" class="btn btn-success"  onclick="step_2({{sklblOrder.id}})">Calcul des quantités</button>
            <button type="button" class="btn btn-primary"  onclick="step_3({{sklblOrder.id}})">Génération Fx</button>
            <button type="button" class="btn btn-secondary" disabled>Génération FX/Azure</button>
        {% endif %}
        {% if sklblOrder.sklblStatus == 3 %}
            <button type="button" class="btn btn-success"  onclick="step_1({{sklblOrder.id}})">Intégration fichier client</button>
            <button type="button" class="btn btn-success"  onclick="step_2({{sklblOrder.id}})">Calcul des quantités</button>
            <button type="button" class="btn btn-success"  onclick="step_3({{sklblOrder.id}})">Génération OF</button>
            <button type="button" class="btn btn-primary"  onclick="step_4({{sklblOrder.id}})">Génération Fx</button>
        {% endif %}

        
        <button type="button" class="btn btn-secondary" disabled>Segepar</button>
        <button type="button" class="btn btn-secondary" disabled>Lisage</button>
        
    </div>
</div>